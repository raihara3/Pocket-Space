{
  "name": "sample-web-application",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "test": "jest --config=jest.babel.config.js --silent=false",
    "snapshot": "jest --updateSnapshot",
    "dev": "next dev",
    "build": "next build",
    "start": "next start -p 3000",
    "start-second": "next start -p 3001",
    "p-dev": "run-p dev redis",
    "redis": "docker-compose up redis",
    "docker": "docker-compose up",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "lint": "yarn run eslint",
    "analyze": "cross-env ANALYZE=true next build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.ts?(x)": [
      "eslint --fix"
    ],
    "**/__tests__/**.ts?(x)": [
      "yarn test"
    ]
  },
  "dependencies": {
    "@babel/preset-react": "^7.13.13",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@next/bundle-analyzer": "^10.1.2",
    "@storybook/addon-docs": "^6.2.1",
    "@tailwindcss/postcss7-compat": "^2.0.4",
    "@types/redis": "^2.8.28",
    "@types/socket.io": "^2.1.13",
    "@types/socket.io-client": "^1.4.36",
    "@types/styled-components": "5.1.9",
    "@types/three": "^0.126.2",
    "autoprefixer": "^10.2.5",
    "cross-env": "^7.0.3",
    "eslint-config-airbnb": "^18.2.1",
    "graphql-tag": "^2.11.0",
    "graphql-tools": "^7.0.4",
    "husky": "6.0.0",
    "lint-staged": "^10.5.4",
    "memorystorage": "^0.12.0",
    "next": "10.1.2",
    "next-auth": "^3.13.3",
    "node-mocks-http": "^1.10.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.9",
    "prop-types": "^15.7.2",
    "pusher": "^5.0.0",
    "pusher-js": "^7.0.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-test-renderer": "^17.0.2",
    "redis": "^3.1.0",
    "redis-mock": "^0.56.3",
    "socket.io": "^4.0.1",
    "socket.io-client": "^4.0.1",
    "socket.io-mock": "^1.3.1",
    "socket.io-redis": "^6.1.0",
    "styled-components": "5.2.3",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.0.4",
    "three": "^0.127.0",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.3",
    "typescript-styled-plugin": "^0.15.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-typescript": "^7.13.0",
    "@storybook/addon-actions": "^6.2.1",
    "@storybook/addon-essentials": "^6.2.1",
    "@storybook/addon-links": "^6.2.1",
    "@storybook/react": "^6.2.1",
    "@types/jest": "^26.0.22",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "eslint": "^7.23.0",

    "lint": "yarn run eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.ts?(x)": [
      "eslint --fix"
    ],
    "**/__tests__/**.ts?(x)": [
      "yarn test"
    ]
  },

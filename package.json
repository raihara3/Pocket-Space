    "@types/socket.io": "^2.1.12",
    "@types/socket.io-client": "^1.4.34",
    "@types/styled-components": "^5.1.7",
    "@types/three": "^0.125.3",
    "autoprefixer": "^9.8.6",
    "eslint-config-airbnb": "^18.2.1",
    "graphql-tag": "^2.11.0",
